{
  "name": "netspeak",
  "description": "Netspeak web interface",
  "private": true,
  "scripts": {
    "build": "npx tsc && gatsby build",
    "build:demo": "npx tsc && gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --no-error-on-unmatched-pattern src/** && stylelint \"src/**/*.scss\"",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix src/** && stylelint \"src/**/*.scss\" --fix",
    "test": "npm run build",
    "publish-demo": "npx gulp publishDemo",
    "publish-release": "npx gulp publishRelease"
  },
  "dependencies": {
    "@babel/plugin-transform-spread": "^7.12.1",
    "d3": "^7.0.4",
    "google-protobuf": "^3.13.0",
    "grpc-web": "^1.2.1",
    "node-sass": "^6.0.1",
    "prismjs": "^1.25.0",
    "rc-slider": "^9.7.2",
    "react-select": "^5.0.0",
    "react-switch": "^6.0.0",
    "gatsby": "^3.14.2",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sass": "^4.14.0",
    "postcss": "^8.3.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "reactjs-popup": "^2.0.5"
  },
  "devDependencies": {
    "@types/d3": "^7.0.0",
    "@types/google-protobuf": "^3.7.3",
    "@types/node": "^14.10.2",
    "@types/prismjs": "^1.16.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.0",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "prettier": "2.1.0",
    "shelljs": "^0.8.3",
    "simple-git": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "typescript": "^4.0.2"
  }
}
