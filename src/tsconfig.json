{
	"compilerOptions": {
		"target": "ES5",
		"lib": ["es2015", "dom", "es2016.array.include", "dom.iterable"],
		"module": "esnext",
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,

		"noImplicitAny": false,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"strictNullChecks": false,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true,
		"downlevelIteration": true,

		"allowJs": true,
		"checkJs": true
	}
}
