{
	"compilerOptions": {
		"target": "ES5",
		"lib": ["es2015", "dom", "es2016.array.include", "dom.iterable"],
		"module": "esnext",

		"noImplicitAny": false,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"strictNullChecks": false,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": false,
		"downlevelIteration": true,

		"allowJs": true,
		"checkJs": true,
		"noEmit": true
	},
	"include": [
		"netspeak-app/**/*.js",
		"pages/**/*.js",
	]
}
