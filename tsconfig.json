{
	"compilerOptions": {
		"target": "ES5",
		"lib": ["es2015", "ES2016", "DOM"],
		"module": "CommonJS",

		"noImplicitAny": false,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true,
		"downlevelIteration": true,

		"allowJs": true,
		"checkJs": true,
		"noEmit": true
	},
	"include": [
		"./gulpfile.js"
	],
	"exclude": [
		"node_modules/**"
	]
}
